{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/mcha0181/data-visualisation-2/refs/heads/main/australia_cuisine_revenue_2000_2025.csv",
    "format": {"type": "csv"}
  },
  "mark": {
    "type": "line",
    "interpolate": "monotone",
    "strokeWidth": 2
  },
  "encoding": {
    "x": {"field": "Year", "type": "temporal", "title": "Year"},
    "y": {"field": "Revenue (Million AUD)", "type": "quantitative", "title": "Revenue (Million AUD)"},
    "color": {"field": "Cuisine", "type": "nominal", "title": "Cuisine Type"},
    "opacity": {"value": 0.7}
  },
  "config": {
    "view": {"stroke": null},
    "background": "#f8fbff",
    "axis": {"labelFontSize": 12, "titleFontSize": 14}
  }
}
